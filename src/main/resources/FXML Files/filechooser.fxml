<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="164.0" prefWidth="434.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unicam.pa2021.filippolampa.view.FileChooserController">
   <children>
      <Label layoutX="114.0" layoutY="14.0" text="Insert the path to yout logo program">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="pathTextField" layoutX="88.0" layoutY="70.0" prefHeight="25.0" prefWidth="260.0" />
      <Button fx:id="confirmButton" layoutX="227.0" layoutY="125.0" mnemonicParsing="false" onAction="#handleConfirm" text="Confirm" />
      <Button fx:id="cancelButton" layoutX="151.0" layoutY="125.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="25.0" prefWidth="59.0" text="Cancel" />
   </children>
</AnchorPane>
